<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<AnchorPane prefHeight="540.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jflat.JFlatController">
   <children>
      <ListView id="wordsList" fx:id="wordsList" layoutX="82.0" layoutY="44.0" onKeyPressed="#getWordDef" onMouseClicked="#getWordDef" prefHeight="458.0" prefWidth="170.0" />
      <WebView id="defView" fx:id="defView" layoutX="261.0" layoutY="44.0" prefHeight="458.0" prefWidth="684.0" />
      <TextField fx:id="autoCompleteField" layoutX="82.0" layoutY="14.0" onKeyTyped="#autoCompleteListener" prefHeight="27.0" prefWidth="170.0" />
      <Button fx:id="switchBTN" layoutX="82.0" layoutY="505.0" mnemonicParsing="false" onAction="#switchDict" prefHeight="27.0" prefWidth="32.0">
         <graphic>
            <FontIcon iconLiteral="ion4-ios-swap" iconSize="20" />
         </graphic></Button>
      <Button fx:id="ttsBTN" layoutX="187.0" layoutY="505.0" mnemonicParsing="false" onMouseClicked="#ttsPlay">
         <graphic>
            <FontIcon fontSmoothingType="LCD" iconLiteral="ion4-ios-play" iconSize="20" />
         </graphic></Button>
      <Button fx:id="delBTN" layoutX="155.0" layoutY="505.0" mnemonicParsing="false" onMouseClicked="#deleteWord">
         <graphic>
            <FontIcon iconLiteral="ion4-ios-close" iconSize="20" />
         </graphic></Button>
      <Button fx:id="addBTN" layoutX="123.0" layoutY="505.0" mnemonicParsing="false">
         <graphic>
            <FontIcon iconLiteral="ion4-ios-add" iconSize="20" />
         </graphic></Button>
      <Button fx:id="closeBTN" layoutX="910.0" layoutY="13.0" mnemonicParsing="false" onAction="#handleCloseBTN">
         <graphic>
            <FontIcon iconLiteral="mdi-close" iconSize="20" />
         </graphic>
      </Button>
      <Button fx:id="maximizeBTN" layoutX="867.0" layoutY="13.0" mnemonicParsing="false">
         <graphic>
            <FontIcon iconLiteral="mdi-window-maximize" iconSize="20" />
         </graphic>
      </Button>
      <Button fx:id="minimizeBTN" layoutX="823.0" layoutY="13.0" mnemonicParsing="false" onAction="#handleMinimizeBTN">
         <graphic>
            <FontIcon iconLiteral="mdi-window-minimize" iconSize="20" />
         </graphic>
      </Button>
   </children>
</AnchorPane>
